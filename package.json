{
  "name": "frontend-task",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.1",
    "styled-components": "^5.3.3",
    "ts-jest": "^24.0.2",
    "typescript": "3.5.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/tests/setup/test-shim.js",
      "<rootDir>/src/tests/setup/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "preset": "ts-jest/presets/js-with-ts",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/setup/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/tests/setup/styleMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testMatch": [
      "**/tests/**/*.test.(ts|tsx|js)"
    ],
    "collectCoverage": false
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1"
  }
}
